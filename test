question = {
                    id: curr._id,
                    type: 'text',
                    value: curr.question.title,
                  }
                }

                if (curr.questionType === 'select') {
                  question = {
                    id: curr._id,
                    type: 'select',
                    title: curr.question.title,
                    allowNewItem: false,
                    editableItems: false,
                    allowMultiselectToggle: false,
                    selectItems: curr.question.options.map((option) => ({
                      id: option._id,
                      value: option.option,
                    })),
                  }
                }

                if (curr.questionType === 'rate') {
                  question = {
                    id: curr._id,
                    type: 'rate',
                    title: curr.question.title,
                    columns: curr.question.columnOptions.map((col) => ({
                      id: col._id,
                      value: col.column,
                    })),
                    rows: curr.question.rowOptions.map((row) => ({
                      id: row._id,
                      value: row.row,
                    })),
                  }
                }